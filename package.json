{
    "name": "fastify-ecommerce-api",
    "version": "1.0.0",
    "description": "Fastify ecommerce API demo project",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/main.ts",
        "build": "tsc -p tsconfig.json",
        "start": "tsc -p tsconfig.json && node build/main.js",
        "lint": "eslint . --ext .ts && prettier --check . && tsc -p tests/tsconfig.json --noEmit",
        "lint:fix": "prettier --write . && eslint --fix",
        "db:seed": "tsx database/seeders/databaseSeeder.ts",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:reset": "prisma migrate reset --force",
        "test": "vitest --run",
        "test:watch": "vitest"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.13.1",
    "dependencies": {
        "@fastify/cookie": "^11.0.2",
        "@fastify/jwt": "^10.0.0",
        "@fastify/swagger": "^9.5.1",
        "@fastify/swagger-ui": "^5.2.3",
        "@prisma/client": "^6.16.2",
        "bcryptjs": "^3.0.2",
        "fastify": "^5.6.0",
        "fastify-plugin": "^5.0.1",
        "fastify-type-provider-zod": "^6.0.0",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@faker-js/faker": "^10.0.0",
        "@types/node": "^24.5.2",
        "@types/supertest": "^6.0.3",
        "eslint": "^9.36.0",
        "globals": "^16.4.0",
        "jiti": "^2.6.0",
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prisma": "^6.16.2",
        "supertest": "^7.1.4",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.44.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    }
}
